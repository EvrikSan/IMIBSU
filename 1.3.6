#include <iostream>

using namespace std;

int main() {
	setlocale(LC_ALL, "Russian");
	int mx = -1e9, i = 0, ansi = -1, x;                //mx - текущий максимум, i - номер текущего числа, ansi - номер текущего максимума, х - текущее число
	while (cin >> x) {
		if (x == 0) {                                  //проверка на конец последовательности
			cout << "Последнее наибольшее число под номером " << ansi << endl;    //ответ
			break;                                     //выход из цикла
		}
		if (x >= mx) {                                 //обновление последнего наибольшего числа
			mx = x;
			ansi = i + 1;
		}
		i++;                                           //подготовка к следующему числу
	}
	return 0;                      //завершение программы
}
